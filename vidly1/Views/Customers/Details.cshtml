@model vidly1.Models.Customer
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
   
<ul>
    <li>Membership type: @Model.MembershipType.MembershipName</li>
    @if (@Model.BirthDate.HasValue)
    {
        <li>Birthday: @Model.BirthDate</li>
    }
</ul>

