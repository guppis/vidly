@model vidly1.Models.Movie
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>

<ul>
    <li>Genre: @Model.Genre</li>
    <li>Release date: @Model.ReleaseDate</li>
    <li>Added date: @Model.DateAdded</li>
    <li>Stock: @Model.NumberAvailable</li>
</ul>

